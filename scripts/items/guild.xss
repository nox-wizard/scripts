SECTION ITEM $item_a_guildstone_deed
{
    NAME a guildstone deed
    ID 14F0
    TYPE $itype_guildstone
    VALUE 35000
    RESTOCK 10
}

SECTION ITEM $item_a_new_guildstone
{
    NAME a guildstone
    ID 0ED5
    VALUE 500000
    RESTOCK 5
    @ONCLICK guild_sclickStone
    @ONDBLCLICK guild_dclickStone
}

SECTION ITEM $item_a_new_guildstone_deed
{
    NAME a guildstone deed
    ID 14F0
    VALUE 35000
    RESTOCK 10
    MOREX $item_a_new_guildstone
    @ONDBLCLICK guild_dclickDeed
}
